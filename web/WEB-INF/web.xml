<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">



        <!--配置selvet-->
        <servlet>
            <servlet-name>ExaminationServlet</servlet-name>
            <servlet-class>web.manager.ExaminationServlet</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>RecordServlet</servlet-name>
            <servlet-class>web.manager.RecordServlet</servlet-class>
        </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>web.client.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>web.manager.UserServlet</servlet-class>
    </servlet>


        <servlet-mapping>
            <servlet-name>ExaminationServlet</servlet-name>
            <url-pattern>/manager/ExaminationServlet</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>RecordServlet</servlet-name>
            <url-pattern>/manager/RecordServlet</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>LoginServlet</servlet-name>
            <url-pattern>/client/LoginServlet</url-pattern>
        </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/manager/UserServlet</url-pattern>
    </servlet-mapping>
</web-app>
